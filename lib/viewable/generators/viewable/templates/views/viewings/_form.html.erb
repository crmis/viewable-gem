<%%= form_for([@<%=viewable_views_name.singularize%>, @viewing]) do |f| %>
  <p>
    <%%= f.label 'start_time', 'Start time' %>
    <%%= f.datetime_select :start_time, { minute_step: 15 } %>
  </p>
  <p>
    <%%= f.label 'length', 'Length of viewing in hours' %>
    <%%= f.number_field 'length', min: 1 %>
  </p>
  <%%= f.submit 'Submit' %>
<%% end %>
